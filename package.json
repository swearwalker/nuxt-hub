{
  "name": "nuxt-hub",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --ext .js,.ts,.vue . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.14.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxt/ui": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/supabase": "^1.5.2",
    "eslint": "^9.29.0",
    "next-auth": "~4.21.1",
    "nuxt": "^3.17.5",
    "typescript": "^5.8.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint:fix",
      "npm run format"
    ]
  }
}
